<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="harkkaknarhi.FXMLPackageCreatorController">
   <children>
      <ComboBox fx:id="thingList" layoutX="429.0" layoutY="287.0" prefWidth="150.0" promptText="valitse esine" />
      <Label fx:id="packageInfo" layoutX="14.0" layoutY="14.0" prefHeight="259.0" prefWidth="299.0" wrapText="true" />
      <ComboBox fx:id="classList" layoutX="429.0" layoutY="324.0" prefWidth="150.0" promptText="Valitse pakettiluokka" />
      <Label fx:id="infoLabel" layoutX="326.0" layoutY="26.0" prefHeight="270.0" prefWidth="253.0" />
      <Button fx:id="addBtn" layoutX="326.0" layoutY="287.0" mnemonicParsing="false" onAction="#addThing" prefHeight="25.0" prefWidth="91.0" text="Esineen tiedot" />
      <Button fx:id="addBox" layoutX="326.0" layoutY="324.0" mnemonicParsing="false" onAction="#setPkg" text="Paketin tiedot" />
      <Button fx:id="createPKG" layoutX="429.0" layoutY="361.0" mnemonicParsing="false" onAction="#exportPkg" prefHeight="25.0" prefWidth="150.0" text="Luo paketti" />
   </children>
</AnchorPane>
